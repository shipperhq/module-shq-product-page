<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="shipperhq_checkout" type="host">assets.shipperhq.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="shipperhq_checkout" type="host">assets.shipperhq.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="shipperhq_checkout" type="host">rms.shipperhq.com</value>
                <value id="shipperhq_checkout_wss" type="host">wss://rms.shipperhq.com/</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
